config EXAMPLES_RDP
	bool "Read Protect example"
	default n
	depends on ARCH_CHIP_AMEBAD
	depends on AMEBAD_TRUSTZONE
	depends on SECURITY_LINK
	---help---
		Enable the Read Protect example

config USER_ENTRYPOINT
	string
	default "rdp_main" if ENTRY_RDP
